{
  "name": "fibrelite",
  "version": "2.0.0",
  "source": "fibrelite.js",
  "main": "dist/fibrelite.js",
  "module": "dist/fibrelite.m.js",
  "scripts": {
    "build": "microbundle",
    "watch": "microbundle watch --format umd",
    "test": "karmatic",
    "serve": "live-server"
